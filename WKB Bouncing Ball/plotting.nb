       SuperscriptBox["\[HBar]", "2"], 
       RowBox[{"2", "m"}]]}], 
     RowBox[{
      RowBox[{"\[Psi]", "''"}], "[", "x", "]"}]}], "+", 
    RowBox[{"m", " ", "g", " ", "x", " ", 
     RowBox[{"\[Psi]", "[", "x", "]"}]}]}], "==", 
   RowBox[{"\[ScriptCapitalE]", " ", 
    RowBox[{"\[Psi]", "[", "x", "]"}]}]}]}]], "Input",
