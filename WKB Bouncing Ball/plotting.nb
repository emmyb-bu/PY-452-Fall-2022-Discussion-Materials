    RowBox[{
     SuperscriptBox["\[HBar]", "2"], " ", 
     RowBox[{
      SuperscriptBox["\[Psi]", "\[Prime]\[Prime]",
       MultilineFunction->None], "[", "x", "]"}]}], 
    RowBox[{"2", " ", "m"}]]}], "\[Equal]", 
  RowBox[{"\[ScriptCapitalE]", " ", 
   RowBox[{"\[Psi]", "[", "x", "]"}]}]}]], "Output",
